# Environment variables for EXAIM
# Copy this file to .env and fill in your API keys

# LLM Provider Configuration
MAS_LLM_PROVIDER=google
MAS_LLM_MODEL=gemini-2.0-flash-exp

# Google API Key (required for Gemini models in MAS)
GOOGLE_API_KEY=your_google_api_key_here

# vLLM Server Configuration (internal only, not exposed publicly)
# Points to localhost vLLM server for summarizer and buffer_agent roles
OPENAI_BASE_URL=http://localhost:8001/v1
OPENAI_API_KEY=EMPTY

# Optional: Other providers
# GROQ_API_KEY=your_groq_api_key_here
